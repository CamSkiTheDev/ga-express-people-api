{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "include": ["."],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(node_modules/)"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./docs/"
  }
}
